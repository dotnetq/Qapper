<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Martin MacRobert (dev@macrobert.com)</Authors>
    <Company>MacRobert.com</Company>
    <Description>Qapper (pronounced 'kwapper') is a super simple object mapper for kdb+\q modelled after Dapper</Description>
    <Copyright>macrobert.com Â© 2018</Copyright>
    <PackageLicenseUrl>https://github.com/dotnetq/Qapper/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/dotnetq/Qapper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dotnetq/Qapper.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>kdb+\q ORM C#</PackageTags>
    <Version>1.0.5</Version>
    <PackageReleaseNotes>Move to DotnetQ organization</PackageReleaseNotes>
    <RootNamespace>DotnetQ.Qapper</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="QSchema.Attributes" Version="1.0.8" />
    <PackageReference Include="qsharp.netstandard" Version="2.3.1" />
  </ItemGroup>

</Project>
